<table class="table">
    <caption>Cтатистика денег по компаниям</caption>
    <tr>
        <th>Название опер / модератора</th>
        <th>мар</th>
        <th>апр</th>
        <th>май</th>
        <th>июн</th>
        <th>июл</th>
        <th>авг</th>
    </tr>
    {% set s1 = 0 %}
    {% set s2 = 0 %}
    {% set s3 = 0 %}
    {% set s4 = 0 %}
    {% set s5 = 0 %}
    {% set s6 = 0 %}
    {% for moc in moneyOfCompany  %}
        <tr>
            <td>{{ moc.title }}</td>
            <td>{{ moneyOfCompany1[moc.title] is defined ? moneyOfCompany1[moc.title].sumprice : 0}} {% set s1 = s1 + (moneyOfCompany1[moc.title] is defined ? moneyOfCompany1[moc.title].sumprice : 0) %}</td>
            <td>{{ moneyOfCompany2[moc.title] is defined ? moneyOfCompany2[moc.title].sumprice : 0}} {% set s2 = s2 + (moneyOfCompany2[moc.title] is defined ? moneyOfCompany2[moc.title].sumprice : 0) %}</td>
            <td>{{ moneyOfCompany3[moc.title] is defined ? moneyOfCompany3[moc.title].sumprice : 0}} {% set s3 = s3 + (moneyOfCompany3[moc.title] is defined ? moneyOfCompany3[moc.title].sumprice : 0) %}</td>
            <td>{{ moneyOfCompany4[moc.title] is defined ? moneyOfCompany4[moc.title].sumprice : 0}} {% set s4 = s4 + (moneyOfCompany4[moc.title] is defined ? moneyOfCompany4[moc.title].sumprice : 0) %}</td>
            <td>{{ moneyOfCompany5[moc.title] is defined ? moneyOfCompany5[moc.title].sumprice : 0}} {% set s5 = s5 + (moneyOfCompany5[moc.title] is defined ? moneyOfCompany5[moc.title].sumprice : 0) %}</td>
            <td>{{ moneyOfCompany6[moc.title] is defined ? moneyOfCompany6[moc.title].sumprice : 0}} {% set s6 = s6 + (moneyOfCompany6[moc.title] is defined ? moneyOfCompany6[moc.title].sumprice : 0) %}</td>
        </tr>
    {% endfor %}
    <tr>
        <td></td>
        <td>{{ s1 }}</td>
        <td>{{ s2 }}</td>
        <td>{{ s3 }}</td>
        <td>{{ s4 }}</td>
        <td>{{ s5 }}</td>
        <td>{{ s6 }}</td>
    </tr>
    <tr>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td><b>ИТОГО</b></td>
        <td><b>{{ s1 + s2 + s3 + s4 + s5 + s6 }}</b></td>
    </tr>
</table>