<table>
    <tr>
        <td>
            {% if company.logo.path is defined %}
                <div style="margin-left: 50px; padding-top: 5px">
                    <img src="{{ company.logo.path }}" style="max-width: 120px; max-height: 40px">
                </div>
            {% endif %}
        </td>
        <td>
            <div style="margin-left: 50px; padding-top: 5px">
                <span style="color: #FFF">{{ company.title }}</span>
            </div>
        </td>
    </tr>
</table>
