{% extends 'CrmAuthBundle::layout.html.twig' %}



{% block content %}
        <form style="max-width: 700px">
            <table class="table">
                    <tr>
                        <td>
                            <table>
                                <tr style="{{ user.typeCard > 0 ? 'background: #FFBCBC' : '' }}">
                                    <td style="{{ user.typeCard > 0 ? 'background: #FFBCBC' : '' }}">
                                        <div class="form-group">
                                            <label class="control-label col-sm-3" for="typeCard">Тип выдачи:</label>
                                            <div class="col-sm-3">
                                                        {{ user.typeCard == 0 ? 'Первичная выдача карты' : '' }}
                                                        {{ user.typeCard == 1 ? 'Замена в связи с истечением срока действия карты' : '' }}
                                                        {{ user.typeCard == 2 ? 'Замена в связи с дефектом, утерей или утратой карты' : '' }}
                                                        {{ user.typeCard == 3 ? 'Замена карты вследствие изменения персональных данных' : '' }}
                                            </div>
                                        </div>
                                    </td>
                                    <td>
                                        <div class="form-group">
                                            <label class="control-label col-sm-3" for="oldNumber">Старая карта:</label>
                                            <div class="col-sm-9">
                                                <input disabled="disabled" type="text"  name="oldNumber" id="oldNumber" class="form-control" value="{{ user.lastNumberCard }}">
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="status">Компания</label>
                            <div class="col-sm-9">
                                {{ user.company.title }}
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="status">Статус</label>
                            <div class="col-sm-9">
                                {{ user.statusString }}
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="type">Тип карты</label>
                            <div class="col-sm-9">

                                    {{ user.ru == 0 and user.estr == 0 ? 'СКЗИ' : ''}}
                                    {{ user.ru == 0 and user.estr == 1 ? 'ЕСТР' : ''}}
                                    {{ user.ru == 1 and user.estr == 0 ? 'РФ' : ''}}

                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="price">Стоимость:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="price" id="price" class="form-control" value="{{ user.price }}" style="width: 200px">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="h2">
                            Данные для связи
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="username">Телефон:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="username" id="username" class="form-control" value="{{ user.username }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="email">E-mail:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="email" id="email" class="form-control" value="{{ user.email }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="h2">
                            Паспортные данные
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="lastName">Сканы паспорта:</label>
                            <div class="col-sm-4">
                                {% if user.copyPassport.path is defined %}
                                    <img src="/{{ user.copyPassport.path }}" brightness="128" contrast="128" data-type="passportFile" class="document" id="passportFileImg">
                                    <br>
                                {% endif %}
                            </div>
                            {% if user.ru == 1 or user.estr == 1 %}
                                <div class="col-sm-1"></div>
                                <div class="col-sm-4">
                                    {% if user.copyPassport2.path is defined %}
                                        <img src="/{{ user.copyPassport2.path }}" brightness="128" contrast="128" data-type="passport2File" class="document" id="passport2FileImg">
                                        <br>

                                    {% endif %}
                                </div>
                            {% endif %}
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="lastName">Фамилия:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="lastName" id="lastName" class="form-control" value="{{ user.lastName }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="firstName">Имя:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="firstName" id="firstName" class="form-control" value="{{ user.firstName }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="surName">Отчество:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="surName" id="surName" class="form-control" value="{{ user.surName }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="birthDate">Дата рождения:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="birthDate" id="birthDate" class="form-control date" value="{{ user.birthDate|date('d.m.Y') }}">
                            </div>
                        </div>
                    </td>
                </tr>

                {% if user.ru == 0 and user.estr == 0 %}
