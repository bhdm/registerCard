{% extends 'CrmMainBundle::layout.html.twig'  %}

{% block title %}
    <title>Заказать карту предприятия тахографа с СКЗИ, РФ. ЕСТР Online</title>
    <meta name="description" content="Быстро и удобно заказать карту предприятия для тахографов">
    <meta name="keywords" content="Заказать карту водителя, тахографы, СКЗИ, ЕСТР, РФ, онлайн продажа карт для тахографа">
{% endblock %}

{% block content %}
    <h2 style="text-align: center">Карта СКЗИ</h2>
    <p style="text-align: center">
        Карты предназначены для тахографов оборудованных блоком СКЗИ.<br />
        Действуют <b>только на территории РФ.</b>
    </p>
    <form method="POST" action="{{ path('application_company') }}" enctype="multipart/form-data">
        <div class="block-form">
            {{ form_errors(form) }}
            <table class="table-form">
                 <tr>
                    <td>Email<span class="required">*</span></td>
                    <td>{{ form_widget(form.email) }}</td>
                    <td class="typeCardFile">Заявление на изменение<span class="mini">(допустимые форматы: PDF,JPG,PNG)</span></td>
                </tr>
                <tr>
                    <td>Телефон<span class="required">*</span></td>
                    <td>{{ form_widget(form.username) }}</td>
                    <td class="typeCardFile">
                        {{ form_widget(form.typeCardFile) }}
                    </td>
                </tr>
                <tr>
                    <td>{{ form_label(form.citizenship)}}</td>
                    <td>
                        {{ form_widget(form.citizenship) }}
                    </td>
                    <td class="citizenship">Временная регистрация<span class="mini">(допустимые форматы: PDF,JPG,PNG)</span></td>
                </tr>
                <tr>
                    <td>Тип заявления<span class="required">*</span></td>
                    <td>
                        {{ form_widget(form.typeCard) }}
                    </td>
                    <td class="citizenship">
                        {{ form_widget(form.copyWork) }}
                    </td>
                </tr>
                <tr class="typeCardFileRow">
                    <td>{{ form_label(form.lastNumberCard) }}<span class="required">*</span></td>
                    <td>{{ form_widget(form.lastNumberCard) }}</td>
                </tr>
                {#<tr>#}
                    {#<td>Количество карт<span class="required">*</span></td>#}
                    {#<td>{{ form_widget(form.cardAmount) }}</td>#}
                    {#<td>#}
                        {#{{ form_widget(form.fileOrderTwo) }}#}
                    {#</td>#}
                {#</tr>#}
            </table>
        </div>
        <hr>
        <div class="block-form" >
            <table class="table-form">
                <caption>Паспортные данные</caption>
                <tr>
                    <td>Фамилия<span class="required">*</span></td>
                    <td>{{ form_widget(form.lastName) }}</td>
                    <td>Копия паспорта <span class="mini">(допустимые форматы: PDF,JPG,PNG)</span></td>
                </tr>
                <tr>
                    <td>Имя<span class="required">*</span></td>
                    <td>{{ form_widget(form.firstName) }}</td>
                    <td rowspan="7" style="vertical-align: top">
                        <div class="file-container">
                            <input type="file" name="passportFile" id="passportFile" class="fileAjax"  required="required">
                            <progress class="progress" value="0" max="100" style="width: 270px"></progress>
                            <br />
                            <br />
