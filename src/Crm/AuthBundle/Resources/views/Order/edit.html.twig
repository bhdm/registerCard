                        <td>
                            <div class="form-group">
                                <label class="control-label col-sm-3" for="surName">Серия и номер:</label>
                                <div class="col-sm-4">
                                    <input disabled="disabled" type="text"  name="passportSerial" id="passportSerial" class="form-control" value="{{ user.passportSerial }}">
                                </div>
                                <div class="col-sm-5">
                                    <input disabled="disabled" type="text"  name="passportNumber" id="passportNumber" class="form-control" value="{{ user.passportNumber }}">
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="form-group">
                                <label class="control-label col-sm-3" for="surName">Кем выдан:</label>
                                <div class="col-sm-9">
                                    <input disabled="disabled" type="text"  name="PassportIssuance" id="PassportIssuance" class="form-control" value="{{ user.PassportIssuance }}">
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="form-group">
                                <label class="control-label col-sm-3" for="surName">Дата выдачи:</label>
                                <div class="col-sm-9">
                                    <input disabled="disabled" type="text"  name="PassportIssuanceDate" id="PassportIssuanceDate" class="form-control date" value="{{ user.PassportIssuanceDate|date("d.m.Y") }}">
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="form-group">
                                <label class="control-label col-sm-3" for="surName">Код подразделения:</label>
                                <div class="col-sm-9">
                                    <input disabled="disabled" type="text"  name="passportCode" id="passportCode" class="form-control code" value="{{ user.passportCode }}">
                                </div>
                            </div>
                        </td>
                    </tr>

                {% endif %}

                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="region">Регион:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="region" id="region" class="form-control" value="{{ user.registeredRegion }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="area">Район:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="area" id="area" class="form-control" value="{{ user.registeredArea }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="city">Город:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="city" id="city" class="form-control" value="{{ user.registeredCity }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="street">Улица:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="street" id="street" class="form-control"  value="{{ user.registeredStreet }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="house">Здание:</label>
                            <div class="col-sm-2">
                                <input disabled="disabled" type="text"  name="house" id="house" class="form-control" placeholder="Дом"  value="{{ user.registeredHome }}">
                            </div>
                            <div class="col-sm-2">
                                <input disabled="disabled" type="text"  name="corp" id="corp" class="form-control" placeholder="Корп"  value="{{ user.registeredCorp }}">
                            </div>
                            <div class="col-sm-2">
                                <input disabled="disabled" type="text"  name="structure" id="structure" class="form-control" placeholder="Стр" value="{{ user.registeredStructure }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="room">Квартира:</label>
                            <div class="col-sm-3">
                                <input disabled="disabled" type="text"  name="room" id="room" class="form-control" value="{{ user.registeredRoom }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="zipcode">Индекс:</label>
                            <div class="col-sm-3">
                                <input disabled="disabled" type="text"  name="zipcode" id="zipcode" class="form-control" value="{{ user.registeredZipcode }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="h2">
                            Водительское удостоверение
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="lastName">Сканы документа:</label>
                            <div class="col-sm-4">
                                {% if user.copyDriverPassport.path is defined %}
                                    <img src="/{{ user.copyDriverPassport.path }}" brightness="0" contrast="0" data-type="driverFile" class="document" id="driverFileImg">
                                    <br>
                                {% endif %}
                            </div>
                            <div class="col-sm-1"></div>
                            {% if user.ru == 1 or user.estr == 1 %}
                                <div class="col-sm-4">
                                    {% if user.copyDriverPassport2.path is defined %}
                                        <img src="/{{ user.copyDriverPassport2.path }}" brightness="0" contrast="0" data-type="driver2File" class="document" id="driver2FileImg">
                                        <br>
                                    {% endif %}
                                </div>
                            {% endif %}
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="driverPlace">Кем выдано:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="driverPlace" id="driverPlace" class="form-control" value="{{ user.driverDocIssuance }}"  maxlength="33">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="driverNumber">Номер удоств.:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="driverNumber" id="driverNumber" class="form-control" value="{{ user.driverDocNumber }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="driverStarts">Дата выдачи:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="driverStarts" id="driverStarts" class="form-control date" value="{{ user.driverDocDateStarts|date('d.m.Y') }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="h2">
                            Фотография и подпись
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="lastName">Фото и подпись:</label>
                            <div class="col-sm-4">
                                {% if user.photo.path is defined %}
                                    <img src="/{{ user.photo.path }}" brightness="0" contrast="0" data-type="photoFile" class="document" id="photoFileImg">
                                    <br>
                                {% endif %}
                            </div>
                            <div class="col-sm-1"></div>
                            <div class="col-sm-4">
                                {% if user.copySignature.path is defined %}
                                    <img src="/{{ user.copySignature.path }}" brightness="0" contrast="0" data-type="signFile" class="document" id="signFileImg">
                                    <br>
                                {% endif %}
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="snils">Снилс:</label>
                            <div class="col-sm-4">
                                <input disabled="disabled" type="text"  name="snils" id="snils" class="form-control snils" value="{{ user.snils }}">
                            </div>
                            <div class="col-sm-1"></div>
                            <div class="col-sm-4">
                                {% if user.copySnils.path is defined %}
                                    <img src="/{{ user.copySnils.path }}" brightness="0" contrast="0" data-type="snilsFile" class="document" id="snilsFileImg">
                                    <br>
                                {% else %}
                                    <img src="" brightness="0" contrast="0" data-type="snilsFile" class="document" id="snilsFileImg">
                                {% endif %}
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="h2">
                            Информация о доставке
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="deliveryRegion">Регион:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="deliveryRegion" id="deliveryRegion" class="form-control" value="{{ user.dileveryRegion }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="deliveryArea">Район:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="deliveryArea" id="deliveryArea" class="form-control" value="{{ user.dileveryArea }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="deliveryCity">Город:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="deliveryCity" id="deliveryCity" class="form-control" value="{{ user.dileveryCity }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="deliveryStreet">Улица:</label>
                            <div class="col-sm-9">
                                <input disabled="disabled" type="text"  name="deliveryStreet" id="deliveryStreet" class="form-control" value="{{ user.dileveryStreet }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="deliveryHouse">Здание:</label>
                            <div class="col-sm-2">
                                <input disabled="disabled" type="text"  name="deliveryHouse" id="deliveryHouse" class="form-control" placeholder="Дом" value="{{ user.dileveryHome }}">
                            </div>
                            <div class="col-sm-2">
                                <input disabled="disabled" type="text"  name="deliveryCorp" id="deliveryCorp" class="form-control" placeholder="Корп" value="{{ user.dileveryCorp }}">
                            </div>
                            <div class="col-sm-2">
                                <input disabled="disabled" type="text"  name="deliveryStructure" id="deliveryStructure" class="form-control" placeholder="Стр" value="{{ user.dileveryStructure }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="deliveryRoom">Квартира:</label>
                            <div class="col-sm-3">
                                <input disabled="disabled" type="text"  name="deliveryRoom" id="deliveryRoom" class="form-control" value="{{ user.dileveryRoom }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="deliveryZipcode">Индекс:</label>
                            <div class="col-sm-3">
                                <input disabled="disabled" type="text"  name="deliveryZipcode" id="deliveryZipcode" class="form-control" value="{{ user.dileveryZipcode }}">
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="h2">
                            Дополнительная информация
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="myPetition">Свое ходатайство":</label>
                            <div class="col-sm-9">
                                {{ user.companyPetition != null ? user.companyPetition.title : 'Не использовать' }}
                                {#{% if user.companyPetition != null %}#}
                                    {#<a href="{{ path('panel_petition_edit',{'id' : user.companyPetition.id }) }}" target="_blank">Смотреть</a>#}
                                {#{% endif %}#}
                            </div>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="form-group">
                            <label class="control-label col-sm-3" for="myPetition">Использвоть Ходатайство "НПО Технолог":</label>
                            <div class="col-sm-9">
                                {{ user.myPetition == 0 ? 'Не использовать' : 'Использовать' }}
                            </div>
                        </div>
                    </td>
                </tr>
            </table>
